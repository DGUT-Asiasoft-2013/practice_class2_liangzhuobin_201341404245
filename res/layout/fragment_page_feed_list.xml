    android:layout_height="match_parent"
    android:orientation="vertical" >
    
   <ListView 
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="60dp">
        
    </ListView>
    <FrameLayout 
        android:id="@+id/navbar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
